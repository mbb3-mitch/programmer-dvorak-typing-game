{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-react-jsx",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
      }
    ],
    [
      "import-rename",
      {
        "^(.*)\\.jsx$": "$1"
      }
    ]
  ]
}